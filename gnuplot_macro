# Gnuplot version 5.0 demo of multiplot auto-layout capability
#
#
reset

set style line 11 lc rgb '#808080' lt 1
set border 3 back ls 11
set tics nomirror
set style line 12 lc rgb '#808080' lt 0 lw 1
set grid back ls 12

set style line 1 lc rgb  '#3361ff' pt 7 ps 1 lt 1 lw 2 # --- blue
set style line 2 lc rgb  '#afff33' pt 7 ps 1 lt 1 lw 2 # --- blue
set style line 3 lc rgb  '#33ffa5' pt 7 ps 1 lt 1 lw 2 # --- blue

set xlabel 'time step (fm)'
set ylabel 'Energy (Mev)'
set multiplot layout 1,3 title "ENERGY VARIATIONS FOR ASYMMETERY TESTING B=20 NUM = 1 BENE = 250 MeV" 
set title "TOTAL ENERGY"
unset key
plot '../altered_version_v11/xxx3/xxx3-4/prop_out_4.dat' u 1:2 every 15:15 with linespoints  ls 1 
#
set title "KINETIC ENERGY"
unset key
plot '../altered_version_v11/xxx3/xxx3-4/prop_out_4.dat' u 1:4 every 15:15 with linespoints ls 2
#
set title "ASYMMETRY ENERGY"
unset key
plot '../altered_version_v11/xxx3/xxx3-4/prop_out_4.dat' u 1:7 every 15:15 with linespoints ls 3
unset multiplot
reset
